{
  "name": "tollan-blog",
  "version": "0.0.1",
  "description": "Blog module for tollan applications",
  "author": "Paul Rayes",
  "license": "apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/leoninepublishers/tollan-blog"
  },
  "main": "lib/index.js",
  "browser": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run lint:watch",
    "lint": "eslint lib",
    "lint:watch": "gaze 'eslint $path' 'lib/**/*.js'"
  },
  "browserify": {
    "transform": [
      "babelify",
      [
        "envify",
        {
          "TOLLAN_ENV": "browser"
        }
      ],
      "isbrowser",
      "unreachable-branch-transform"
    ]
  },
  "devDependencies": {
    "gaze-cli": "^0.1.0",
    "eslint": "^0.15.0"
  },
  "dependencies": {
    "babelify": "^5.0.4",
    "bunyan": "^1.3.4",
    "eventemitter3": "^0.1.6",
    "forrrm": "*",
    "gulp-eslint": "^0.12.0",
    "http-slug": "^1.0.0",
    "insignia": "^4.2.2",
    "logule": "^2.0.1",
    "object-assign": "^2.0.0",
    "parent-require": "^1.0.0",
    "promise": "^6.1.0",
    "remarkable": "^1.6.0",
    "shortid": "^2.1.3",
    "tollan-filemanager": "*",
    "unreachable-branch-transform": "^0.2.3",
    "validate.js": "^0.5.0",
    "velocity-animate": "^1.2.2"
  }
}
